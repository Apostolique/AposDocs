{
  "name": "apos-docs",
  "version": "0.2.2",
  "main": "index.js",
  "scripts": {
    "dev": "run-p dev:tmp dev:html dev:css",
    "dev:tmp": "mkdirp _tmp && echo . >_tmp/style.css",
    "dev:html": "eleventy --serve",
    "dev:css": "postcss styles/style.css -o _tmp/style.css --watch",
    "build": "cross-env ELEVENTY_PRODUCTION=true eleventy && cross-env NODE_ENV=production postcss styles/style.css -o _site/style.css && cleancss -o _site/style.css _site/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apostolique/apos-docs.git"
  },
  "bugs": {
    "url": "https://github.com/Apostolique/apos-docs/issues"
  },
  "keywords": [
    "apos",
    "documentation"
  ],
  "author": "Jean-David Moisan",
  "license": "MIT",
  "homepage": "https://github.com/Apostolique/apos-docs#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.2.4",
    "clean-css-cli": "^4.3.0",
    "cross-env": "^7.0.3",
    "domutils": "^2.4.4",
    "fs-extra": "^9.1.0",
    "html-minifier": "^4.0.0",
    "htmlparser2": "^6.0.0",
    "js-yaml": "^3.14.1",
    "markdown-it": "^12.0.4",
    "markdown-it-anchor": "^6.0.1",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-footnote": "^3.0.2",
    "markdown-it-replace-link": "^1.1.0",
    "markdown-it-sub": "^1.0.0",
    "markdown-it-sup": "^1.0.0",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^13.0.0",
    "postcss-nested": "^5.0.3",
    "tailwindcss": "^2.0.2",
    "url-join": "^4.0.1"
  }
}
